{
  "name": "ethpm",
  "version": "0.1.0",
  "description": "Javascript library for EthPM (EIPs 1123 / 1319)",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch"
  },
  "keywords": [
    "ethereum",
    "smart-contract",
    "package",
    "packaging"
  ],
  "author": "g. nicholas d'andrea <gnidan@trufflesuite.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^23.3.1",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/node": "^10.7.1",
    "glob": "^7.1.3",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "tsconfig-paths": "^3.5.0",
    "typedoc": "^0.12.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "deep-equal": "^1.0.1",
    "ethpm-spec": "git+https://github.com/ethpm/ethpm-spec.git#ethpm.js",
    "io-ts": "^1.3.0",
    "ipfs-unixfs": "^0.1.15",
    "ipld-dag-pb": "^0.14.8",
    "json-stable-stringify": "^1.0.1",
    "module-alias": "^2.1.0",
    "original-require": "^1.0.1",
    "source-map-support": "^0.5.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "<rootDir>/tsconfig.json",
        "enableTsDiagnostics": true
      }
    },
    "moduleNameMapper": {
      "^ethpm/(.*)": "<rootDir>/src/$1",
      "^ethpm$": "<rootDir>/src",
      "^test/(.*)": "<rootDir>/test/$1",
      "^types": "<rootDir>/types"
    },
    "testMatch": [
      "**/test/*.+(ts|js)"
    ]
  },
  "_moduleAliases": {
    "ethpm": "dist"
  }
}
