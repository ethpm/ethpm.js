{
  "name": "ethpm",
  "version": "0.1.0-next.12",
  "description": "Javascript library for EthPM (EIPs 1123 / 1319)",
  "main": "index.js",
  "types": "main.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "docs": "gulp docs",
    "build": "gulp",
    "watch": "gulp watch",
    "lint": "gulp lint",
    "cruise:deps": "depcruise --output-type dot --validate .dependency-cruiser.json src  | uniq | dot -T pdf > dependencygraph.pdf"
  },
  "keywords": [
    "ethereum",
    "smart-contract",
    "package",
    "packaging"
  ],
  "author": "g. nicholas d'andrea <gnidan@trufflesuite.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^23.3.1",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/node": "^10.7.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "del": "^3.0.0",
    "dependency-cruiser": "^4.6.3",
    "dir-compare": "^1.8.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "ipfs": "^0.39.0",
    "jest": "^23.5.0",
    "tmp": "^0.1.0",
    "ts-jest": "^23.1.3",
    "tsconfig-paths": "^3.5.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.2.0",
    "typescript-eslint": "0.0.1-alpha.0",
    "vinyl-fs": "^3.0.3",
    "vinyl-paths": "^2.1.0"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "debug": "^3.1.0",
    "deep-equal": "^1.0.1",
    "ethpm-spec": "^2.0.0",
    "io-ts": "^1.3.0",
    "ipfs-http-client": "^27.0.0",
    "ipfs-unixfs": "^0.1.16",
    "ipfsd-ctl": "^0.42.0",
    "ipld-dag-pb": "^0.14.11",
    "json-stable-stringify": "^1.0.1",
    "module-alias": "^2.1.0",
    "original-require": "^1.0.1",
    "source-map-support": "^0.5.9",
    "web3": "1.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "node",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json",
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "moduleNameMapper": {
      "^ethpm/(.*)": "<rootDir>/src/$1",
      "^ethpm$": "<rootDir>/src",
      "^test/(.*)": "<rootDir>/test/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/test/*.+(ts|js)",
      "<rootDir>/test/**/test/*.+(ts|js)",
      "<rootDir>/dist/**/test/*.+(js)"
    ],
    "testEnvironment": "node",
    "preset": "ts-jest"
  },
  "_moduleAliases": {
    "ethpm": "dist",
    "test": "dist/test"
  }
}
