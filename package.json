{
  "name": "ethpm",
  "version": "0.1.0-next.1",
  "description": "Javascript library for EthPM (EIPs 1123 / 1319)",
  "main": "index.js",
  "types": "main.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "docs": "gulp docs",
    "build": "gulp",
    "watch": "gulp watch"
  },
  "keywords": [
    "ethereum",
    "smart-contract",
    "package",
    "packaging"
  ],
  "author": "g. nicholas d'andrea <gnidan@trufflesuite.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^23.3.1",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/node": "^10.7.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "del": "^3.0.0",
    "dts-bundle": "^0.7.3",
    "glob": "^7.1.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^5.0.0-alpha.3",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "tsconfig-paths": "^3.5.0",
    "typedoc": "^0.12.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typescript": "^3.0.1",
    "vinyl-fs": "^3.0.3",
    "vinyl-paths": "^2.1.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "deep-equal": "^1.0.1",
    "ethpm-spec": "git+https://github.com/ethpm/ethpm-spec.git#ethpm.js",
    "io-ts": "^1.3.0",
    "ipfs": "^0.32.3",
    "ipfs-mini": "^1.1.2",
    "ipfs-unixfs": "^0.1.16",
    "ipfsd-ctl": "^0.39.5",
    "ipld-dag-pb": "^0.14.11",
    "json-stable-stringify": "^1.0.1",
    "module-alias": "^2.1.0",
    "original-require": "^1.0.1",
    "source-map-support": "^0.5.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "<rootDir>/tsconfig.json",
        "enableTsDiagnostics": true
      }
    },
    "moduleNameMapper": {
      "^ethpm/(.*)": "<rootDir>/src/$1",
      "^ethpm$": "<rootDir>/src",
      "^test/(.*)": "<rootDir>/test/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/test/*.+(ts|js)",
      "<rootDir>/test/**/test/*.+(ts|js)",
      "<rootDir>/dist/**/test/*.+(js)"
    ]
  },
  "_moduleAliases": {
    "ethpm": "dist",
    "test": "dist/test"
  }
}
